//Convert a list of characters into a String 

// { Driver Code Starts
#include <bits/stdc++.h>
using namespace std;


 // } Driver Code Ends

class Solution{
    public:
    string chartostr(char arr[] , int n)
    {
        string p;
        //code here
        for (int i=0;i<n;i++)
        {
            if (arr[i] != ' ')
            p+=arr[i];
        }
        return p;
    }
};

// { Driver Code Starts.

int main()
{
    int t;
    cin>>t;
    for(int i=0;i<t;i++)
    {
        int n;
        cin>>n;
        char arr[n];
        for(int j=0;j<n;j++)
        {
            cin>>arr[j];
        }
        Solution ob;
        
        cout<<ob.chartostr(arr,n)<<endl;

    }
}
  // } Driver Code Ends
